<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{name}}</title>
    {% if tags|not-empty %}<meta name="keywords" content="{{tags|join:","}}">{% endif %}
    {% if short_description %}<meta name="description" content="{{short_description}}">{% endif %}

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% if theme %}<script src="{{theme|safe}}"></script>{% endif %}
    {% for css in styles %}
    <link rel="stylesheet" href="{{css|safe}}"/>{% endfor %}
    <style>
{{style|safe}}
    </style>
</head>
<body>
<div id="container"></div>
{% for script in scripts %}
<script src="{{script|safe}}"></script>{% endfor %}
<script type="text/javascript">
{{code|safe}}
</script>
</body>
</html>